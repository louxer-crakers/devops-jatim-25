{
  "family": "lks-leantime-app",
  "taskRoleArn": "arn:aws:iam::767398027707:role/LabRole",
  "executionRoleArn": "arn:aws:iam::767398027707:role/LabRole",
  "networkMode": "awsvpc",
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "1024",
  "memory": "2048",
  "volumes": [],

  "placementConstraints": [],
  "runtimePlatform": {
    "cpuArchitecture": "X86_64",
    "operatingSystemFamily": "LINUX"
  },
  "enableFaultInjection": false,

  "containerDefinitions": [
    {
      "cpu": 0,
      "environment": [
        {
          "name": "LEAN_PRIMARY_COLOR",
          "value": "#006d9f"
        },
        {
          "name": "LEAN_USER_FILE_PATH",
          "value": "/var/www/html/userfiles"
        },
        {
          "name": "PUID",
          "value": "1000"
        },
        {
          "name": "LEAN_APP_URL",
          "value": ""
        },
        {
          "name": "LEAN_SITENAME",
          "value": "Leantime"
        },
        {
          "name": "LEAN_SECONDARY_COLOR",
          "value": "#00a886"
        },
        {
          "name": "LEAN_LANGUAGE",
          "value": "en-US"
        },
        {
          "name": "LEAN_DEFAULT_TIMEZONE",
          "value": "Asia/Jakarta"
        },
        {
          "name": "LEAN_APP_DIR",
          "value": ""
        },
        {
          "name": "LEAN_LOGO_PATH",
          "value": "/var/www/html/public/userfiles"
        },
        {
          "name": "LEAN_PORT",
          "value": "8080"
        },
        {
          "name": "LEAN_USE_S3",
          "value": "false"
        },
        {
          "name": "LEAN_DB_BACKUP_PATH",
          "value": "backupdb/"
        },
        {
          "name": "LEAN_DISABLE_LOGIN_FORM",
          "value": "false"
        },
        {
          "name": "LEAN_PRINT_LOGO_URL",
          "value": "/var/www/html/public/userfiles"
        },
        {
          "name": "LEAN_LOG_PATH",
          "value": ""
        },
        {
          "name": "LEAN_DEBUG",
          "value": "0"
        },
        {
          "name": "PGID",
          "value": "1000"
        }
      ],
      "environmentFiles": [],
      "essential": true,
      "image": "leantime/leantime:latest",
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/lks/leantime-app",
          "awslogs-create-group": "true",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "ecs"
        },
        "secretOptions": []
      },
      "mountPoints": [],
      "systemControls": [],
      "ulimits": [],
      "volumesFrom": []
    }
  ]
}
